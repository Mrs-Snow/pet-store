<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mrsnow.petstore.mapper.AddressMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mrsnow.petstore.dao.Address">
        <id column="id" property="id" />
        <result column="USER_ID" property="userId" />
        <result column="CONSIGNEE" property="consignee" />
        <result column="CONSIGNEE_TEL" property="consigneeTel" />
        <result column="DELIVERY_ADDRESS" property="deliveryAddress" />
        <result column="IS_DEFAULT" property="isDefault"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, USER_ID, CONSIGNEE, CONSIGNEE_TEL, DELIVERY_ADDRESS,IS_DEFAULT
    </sql>

</mapper>
