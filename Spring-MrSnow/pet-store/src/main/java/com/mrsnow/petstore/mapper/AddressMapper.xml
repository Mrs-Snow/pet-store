<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mrsnow.petstore.mapper.AddressMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mrsnow.petstore.dao.Address">
        <id column="id" property="id" />
        <result column="USER_ID" property="userId" />
        <result column="收货人" property="收货人" />
        <result column="收货人手机号" property="收货人手机号" />
        <result column="收货地址" property="收货地址" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, USER_ID, 收货人, 收货人手机号, 收货地址
    </sql>

</mapper>
