<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mrsnow.petstore.mapper.CartMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mrsnow.petstore.dao.Cart">
        <id column="id" property="id" />
        <result column="IS_ACCOUNT" property="isAccount" />
        <result column="GOODS_ID" property="goodsId" />
        <result column="GOODS_NAME" property="goodsName" />
        <result column="GOODS_NUM" property="goodsNum" />
        <result column="AMOUNT_MONEY" property="amountMoney" />
        <result column="USER_ID" property="userId" />
        <result column="STORE_ID" property="storeId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, IS_ACCOUNT, GOODS_ID, GOODS_NAME, GOODS_NUM, AMOUNT_MONEY, USER_ID,STORE_ID
    </sql>

</mapper>
