<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mrsnow.petstore.mapper.PreferentialMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mrsnow.petstore.dao.Preferential">
        <id column="id" property="id" />
        <result column="PREFERENTIAL_PRICE" property="preferentialPrice" />
        <result column="PRICE_VALUE" property="priceValue" />
        <result column="STORE_ID" property="storeId" />
        <result column="DISCOUNT" property="discount" />
        <result column="COUNT_VALUE" property="countValue" />
        <result column="COMMENT" property="comment" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, PREFERENTIAL_PRICE, PRICE_VALUE, STORE_ID, DISCOUNT, COUNT_VALUE, COMMENT
    </sql>

</mapper>
